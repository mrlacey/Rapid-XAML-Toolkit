<platformUi:DialogWindow
    x:Class="RapidXamlToolkit.Options.ProfileConfigPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:RapidXamlToolkit"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:options="clr-namespace:RapidXamlToolkit.Options"
    xmlns:platformUi="clr-namespace:Microsoft.VisualStudio.PlatformUI;assembly=Microsoft.VisualStudio.Shell.15.0"
    xmlns:strings="clr-namespace:RapidXaml.Resources"
    Title="{x:Static strings:OptionsStringRes.Options_ProfilePageTitle}"
    MinWidth="300"
    MinHeight="350"
    d:DesignHeight="400"
    d:DesignWidth="500"
    HasHelpButton="False"
    HasMaximizeButton="False"
    HasMinimizeButton="False"
    mc:Ignorable="d">
    <options:ProfileConfigControl x:Name="ProfileConfig" />
</platformUi:DialogWindow>

